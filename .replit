modules = ["nodejs-20"]

[deployment]
run = ["sh", "-c", "PORT=80 npm start"]
build = ["sh", "-c", "npm run build"]

[[ports]]
localPort = 80
externalPort = 80

[workflows]
runButton = "Start Server"

[[workflows.workflow]]
name = "Start Server"
author = 39467490
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "PORT=80 npm run dev"
