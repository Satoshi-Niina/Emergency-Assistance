# ローカル開発環境用設定（例）
# このファイルをコピーして local.env として使用してください

NODE_ENV=development
PORT=8000

# データベース接続（ローカルPostgreSQL）
# 実際のパスワードに変更してください
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD_HERE@localhost:5432/webappdb

# JWT設定（32文字以上のランダムな文字列に変更してください）
JWT_SECRET=your-32-character-secret-key-here-change-this
SESSION_SECRET=your-32-character-session-secret-here-change-this

# フロントエンドURL
FRONTEND_URL=http://localhost:5173

# 信頼プロキシ（ローカルでは不要）
TRUST_PROXY=0

# OpenAI API設定（実際のAPIキーに変更してください）
OPENAI_API_KEY=sk-your-actual-openai-api-key-here
