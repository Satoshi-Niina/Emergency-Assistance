BYPASS_DB_FOR_LOGIN = true
SAFE_MODE = true
JWT_SECRET = emergency-assistance-jwt-secret-2024
SESSION_SECRET = emergency-assistance-session-secret-2024
FRONTEND_URL = https://witty-river-012f39e00.1.azurestaticapps.net
STATIC_WEB_APP_URL = https://witty-river-012f39e00.1.azurestaticapps.net
NODE_ENV = production<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <!-- Minimal configuration for Azure App Service Node.js -->
    <!-- Let Azure App Service handle most of the Node.js detection automatically -->
    <handlers>
      <add name="iisnode" path="azure-server.mjs" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="DynamicContent">
          <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="True"/>
          </conditions>
          <action type="Rewrite" url="azure-server.mjs"/>
        </rule>
      </rules>
    </rewrite>
    <iisnode
      loggingEnabled="true"
      enableXFF="true"
    />
  </system.webServer>
</configuration>
