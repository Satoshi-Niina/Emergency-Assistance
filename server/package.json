{
  "name": "emergency-assistance-server",
  "version": "1.0.3",
  "type": "module",
  "main": "app.js",
  "description": "Emergency Assistance System - Azure App Service Backend",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node azure-server.mjs",
    "start:fallback": "node app.js",
    "start:legacy": "node server.js",
    "start:debug": "node --inspect azure-server.mjs",
    "dev": "cross-env NODE_ENV=development PORT=8080 tsx watch unified-hot-reload-server.js",
    "build": "npm run build:server || echo 'Build had errors but continuing...'",
    "build:server": "tsc --project ../tsconfig.server.json || exit 0",
    "build:oryx-info": "echo 'Oryx runs npm run build, which triggers build:server (TypeScript compilation). This is correct.'",
    "prebuild": "echo Preparing build...",
    "postbuild": "echo Build completed successfully",
    "type-check": "tsc --noEmit --project ../tsconfig.server.json",
    "test": "echo No tests specified"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@azure/identity": "^4.0.1",
    "@azure/storage-blob": "^12.20.0",
    "@azure/storage-file-share": "^12.20.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^10.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.4",
    "emergency-assistance": "file:..",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.104.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.4",
    "sharp": "^0.33.2",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "emergency",
    "assistance",
    "azure",
    "nodejs"
  ],
  "author": "Emergency Assistance Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Satoshi-Niina/Emergency-Assistance.git"
  }
}