module.exports = async (context, req) => {
    context.log('Health check endpoint accessed.');

    context.res = {
        status: 200,
        headers: {
            "Content-Type": "application/json"
        },
        body: {
            status: "ok",
            timestamp: new Date().toISOString(),
            service: "Emergency Assistance API"
        }
    };
};
