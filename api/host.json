{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:05:00",
  "cors": {
    "allowedOrigins": [
      "https://*.azurestaticapps.net",
      "https://*.azurewebsites.net",
      "http://localhost:3000",
      "http://localhost:5173"
    ],
    "supportCredentials": true
  }
}
