{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "rewrite": "https://<your-api>.azurewebsites.net/api/{*}",
      "forwardHeaders": true
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*", "/*.css", "/*.js", "/*.png", "/*.svg", "/*.ico"]
  }
}
