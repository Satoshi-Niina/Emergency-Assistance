{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/images/*",
      "/favicon.ico",
      "/robots.txt",
      "/manifest.webmanifest"
    ]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html", "statusCode": 200 }
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "trailingSlash": "never"
}
