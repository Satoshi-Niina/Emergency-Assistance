{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "esnext",
    "jsx": "react-jsx",
    "types": ["vite/client", "node"],
  "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}