# ローカル開発環境用設定
NODE_ENV=development
PORT=8000

# データベース接続（ローカルPostgreSQL）
DATABASE_URL=postgresql://postgres:password@localhost:5432/emergency_assistance

# JWT設定
JWT_SECRET=dev-jwt-secret-key-32-characters-long
SESSION_SECRET=dev-session-secret-32-characters-long

# フロントエンドURL
FRONTEND_URL=http://localhost:5173

# 信頼プロキシ（ローカルでは不要）
TRUST_PROXY=0