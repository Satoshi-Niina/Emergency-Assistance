  "type": "module",
  "scripts": {
    "dev:clean": "kill-port 3001 3002 5000 5001 5002 5003 && npm install && npm run dev:local",
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\"",
    "dev-client": "cd client && vite --host 0.0.0.0 --port 5001",
    "dev-server": "tsx server/index.ts",
    "dev:client": "cd client && vite --host 0.0.0.0 --port 5000",
    "dev:all": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:backend-dynamic": "tsx server/index.ts",
    "dev:client-dynamic": "cd client && vite --host 0.0.0.0 --port 5000",
    "dev:development": "copy env.development .env && npm run dev",
    "dev:production": "copy env.production .env && npm run dev",
    "dev:dev": "npm run setup:dev && npm run dev",
    "dev:prod": "npm run setup:prod && npm run dev",
    "setup:dev": "copy env.development .env",
    "setup:prod": "copy env.production .env",
    "dev:local": "concurrently \"npm:dev:local-server\" \"npm:dev:local-client\"",
    "dev:local-server": "tsx server/index.dev.ts",
    "dev:local-client": "cd client && vite --config vite.config.dev.ts",
    "dev:local-only": "tsx server/index.dev.ts",
    "dev:client-only": "cd client && vite --config vite.config.dev.ts",
    "dev:ports": "kill-port 3001 3002 5000 5001 5002 5003",
    "dev:setup": "npm run dev:ports && npm run dev:local",
    "build": "npm run build:shared && npm run build:client && npm run build:server && npm run fix:paths",
    "build:shared": "cd shared && npm run build && cd .. && node scripts/copy-shared.js",
    "build:client": "cd client && vite build",
    "build:server": "tsc --project tsconfig.build.json",
    "fix:paths": "node scripts/fix-shared-paths.js",
    "start": "node startup.js",
    "start:prod": "NODE_ENV=production node startup.js",
    "start:development": "copy env.development .env && npm start",
    "verify:deploy": "npm run verify && echo \"✅ デプロイ前確認完了\""
  },
